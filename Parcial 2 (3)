#include "stdafx.h"
#include <iostream>
#include "conio.h"
#define MAX 10000

using namespace std;

void main()
{
	int N, i, j, a[MAX];
	do
	{
		cout<< "Ingrese un numero mayor a 7000: ";
		cin>> N;
	}
	while (N<=7000);
	j=0;
		for (i=1; i<=N; i=i+2)
		{
			a[j]=i;
		    j++;
		}

		getch();
}
